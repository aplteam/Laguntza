 ps.cssScreenFilename←parentFolder,'Files\TestData\Special Screen.css'
 ps.trapErrors←0
 ref←1 ##.Laguntza.New ps
 ref.⎕WS'Event' 9955 1
 ∆Process ref

 :If 0≠≢buffer←##.APLTreeUtils.ReadUtf8File ref.∆tempFilename
 :AndIf 0<+/bool←∨/¨'rel=stylesheet '∘⍷¨buffer
     buffer←(bool⍳1)⊃buffer
     buffer←A.Lowercase buffer
     fn1←{⍵↑⍨⍵⍳'"'}('file:///'{(≢⍺)+1⍳⍨⍺⍷⍵}buffer)↓buffer
     →T.PassesIf≡/A.ReadUtf8File¨fn1 ps.cssScreenFilename
 :EndIf

 R←T._OK
∆TidyUp:
 ##.Laguntza.Reset
⍝Done
